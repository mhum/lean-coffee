<div class="starter-template">
<div class="page-header">
	<h1>Lean Coffee Sessions</h1>
</div>
<div style="padding-bottom: 10px;">
	<div style="text-align: right;">
		<%= link_to "Create New Session", new_session_path, class: "btn btn-success btn-md" %>
	</div>
</div>
<div class = "session-list">
	<table>
	  <% @sessions.each_with_index do |s, idx| %>
	    <tr>
	      <td><%= idx + 1 %>. <%= link_to s.title, session_path(s) %></td>
	    </tr>
	  <% end %>
	</table>
</div>